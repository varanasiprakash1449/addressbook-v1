pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compile Hello World'
            }
        }
        stage('CodeReview') {
            steps {
                echo 'CodeReview Hello World'
            }
        }
        stage('Package') {
            steps {
                echo 'Package Hello World'
            }
        }
    }
}
